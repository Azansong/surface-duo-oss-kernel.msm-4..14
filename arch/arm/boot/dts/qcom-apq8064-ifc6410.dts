#include "qcom-apq8064.dtsi"

/ {
	model = "Qualcomm APQ8064/IFC6410";
	compatible = "qcom,apq8064-ifc6410", "qcom,apq8064";

	memory{
		device_type = "memory";
		reg = <0x80000000 0x80000000>;
	};
	aliases {
		sdcc1 = &sdcc1;
		sdcc3 = &sdcc3;
	};

	soc {
		serial@16640000 {
			status = "ok";
		};
		amba {
			/* eMMC */
			sdcc1: sdcc@12400000 {
				status = "okay";
			};

			/* External micro SD card */
			sdcc3: sdcc@12180000 {
				status = "okay";
			};
		};
	};
};
